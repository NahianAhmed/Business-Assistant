<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="comnahianofficially.fb.businessassistant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".signup" />
        <activity android:name=".Wellcome" />
        <activity android:name=".forgot" />
        <activity android:name=".entryTransaction" />
        <activity android:name=".Income" />
        <activity android:name=".BalanceSheet" />
        <activity android:name=".OwnerEquity"></activity>
    </application>

</manifest>